{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"module": "commonjs",
		"newLine": "lf",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": ".",
		"preserveConstEnums": true,
		"removeComments": true,
		"strict": true
	},
	"compileOnSave": true,
	"include": [
		"src/**/*",
		"types/*.d.ts"
	],
	"exclude": []
}
